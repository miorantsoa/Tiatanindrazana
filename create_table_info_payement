CREATE TABLE info_payement
(
  id INT(11) PRIMARY KEY NOT NULL AUTO_INCREMENT,
  idabonnee INT(11) NOT NULL,
  numero VARCHAR(50),
  trans_id VARCHAR(60),
  CONSTRAINT info_payement_abonnee_idutilisateur2_fk FOREIGN KEY (idabonnee) REFERENCES abonnee (idutilisateur2)
);
CREATE UNIQUE INDEX info_payement_id_uindex ON info_payement (id);
CREATE INDEX info_payement_abonnee_idutilisateur2_fk ON info_payement (idabonnee);